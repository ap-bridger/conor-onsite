generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Greet {
  id Int @id @default(autoincrement())
}

model Transaction {
  id            String   @id @default(cuid())
  date          DateTime
  description   String
  vendor        String
  category      String
  spentCents    Int
  receivedCents Int
  status        String
  comment       String
}
